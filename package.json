{
  "name": "@vips/utils",
  "version": "0.0.1",
  "description": "Front end common tools and methods.",
  "unpkg": "index.umd.js",
  "main": "index.cjs.js",
  "module": "index.js",
  "types": "/",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "mocha",
    "serve": "webpack-dev-server --open --config build/serve.js",
    "build:umd": "cross-env TYPE=umd rollup -c build/build.js",
    "build:cjs": "cross-env TYPE=cjs rollup -c build/build.js",
    "build:es": "cross-env TYPE=es rollup -c build/build.js",
    "build:ts": "tsc",
    "build": "npm run build:ts && npm run gen:entry && npm run build:es && npm run build:umd && npm run build:cjs",
    "gen:entry": "node build/gen-entry.js",
    "publish": "node build/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipway/vipjs-utils.git"
  },
  "keywords": [
    "utils",
    "javascript",
    "es",
    "tools",
    "types"
  ],
  "author": "Cody",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vipway/vipjs-utils/issues"
  },
  "homepage": "https://github.com/vipway/vipjs-utils#readme",
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "dependencies": {
    "@types/clipboard": "^2.0.7",
    "clipboard": "^2.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-external-helpers": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "fse": "^4.0.1",
    "html-webpack-plugin": "^5.4.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-watch": "^4.3.1",
    "shelljs": "^0.8.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "walk": "^2.3.15",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "yargs": "^17.2.1"
  }
